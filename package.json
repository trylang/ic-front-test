  "scripts": {
    "dll": "rimraf dll && webpack --config config/webpack.dll.js --profile --colors --display-error-details",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --colors --profile --hot",
    "startopen": "npm run start -- --open",
