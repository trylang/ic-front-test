    "test": "karma start",
    "e2e": "node_modules/protractor/bin/webdriver-manager update && node_modules/protractor/bin/protractor ./protractor.conf.js",
    "upgrade:core": "npm update @angular/{animations,common,core,forms,http,platform-browser,platform-browser-dynamic,router}"
  },
